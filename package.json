{
  "name": "advanced-react",
  "version": "1.0.0",
  "description": "Advanced React",
  "author": "Daniel Tunde Ojediran",
  "main": "lib/server.js",
  "repository": "https://github.com/tundeojediran/advanced-react.git",
  "license": "MIT",
  "scripts": {
    "dev": "pm2 start lib/server.js --watch --interpreter babel-node",
    "webpack": "webpack -wd",
    "test": "jest --watch"
  },
  "babel": {
    "presets": [
      "react",
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "transform-class-properties",
      "transform-object-rest-spread"
    ]
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "enzyme": "^2.9.1",
    "eslint": "^4.1.0",
    "eslint-plugin-react": "^7.1.0",
    "jest": "^19.0.2",
    "react-test-renderer": "^15.6.1"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "babel-cli": "^6.24.1",
    "babel-loader": "^7.1.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "ejs": "^2.5.6",
    "express": "^4.15.3",
    "lodash.debounce": "^4.0.8",
    "lodash.pickby": "^4.6.0",
    "pm2": "^2.5.0",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-addons-perf": "^15.4.2",
    "react-dom": "^15.6.1",
    "webpack": "^3.0.0"
  }
}
